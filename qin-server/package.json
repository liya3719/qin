{
  "name": "qin-server",
  "version": "1.0.0",
  "private": true,
  "description": "qin server",
  "author": "liya, luoqin",
  "scripts": {
    "complied": "./node_modules/.bin/tsc",
    "start": "npm run complied && cross-env NODE_ENV=prod ./node_modules/.bin/pm2 start server/server.js -i max --name qin-server -e error.log",
    "stop": "./node_modules/.bin/pm2 kill",
    "restart": "npm run stop && npm start",
    "dev": "npm run complied && cross-env NODE_ENV=dev ./node_modules/.bin/devtool server/server.js --watch",
    "serve": "npm run complied && cross-env NODE_ENV=prod node server/server.js"
  },
  "dependencies": {
    "@types/http-errors": "^1.8.0",
    "@types/koa": "^2.0.45",
    "@types/mysql": "^2.15.2",
    "@types/node": "^8.5.2",
    "archiver": "^2.1.1",
    "async": "^2.6.0",
    "config": "^1.28.1",
    "debug": "^2.6.3",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "electron": "10.1.3",
    "fs-extra": "^5.0.0",
    "http-errors": "^1.8.0",
    "jszip": "^3.1.5",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-multer": "^1.0.2",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "mkdirp": "^0.5.1",
    "mysql": "^2.15.0",
    "node-dir": "^0.1.17",
    "node-mysql": "^0.4.2",
    "reflect-metadata": "^0.1.10",
    "request": "^2.87.0",
    "routing-controllers": "^0.7.6",
    "string-format": "^0.5.0",
    "typedi": "^0.6.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "devtool": "^2.3.1",
    "nodemon": "^1.8.1",
    "pm2": "^2.9.1",
    "shelljs": "^0.8.2"
  }
}
